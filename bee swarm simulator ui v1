                main.espsettings.Colors.Ally = a
                main:updatecolors()
            end})
            ui.espsettings:AddColor({text = "Enemy Color", flag = "enemycolor", color = Color3.fromRGB(221, 3, 255), callback = function(a)
                main.espsettings.Colors.Enemy = a
                main:updatecolors()
            end})
