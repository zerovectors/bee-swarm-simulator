local splitline = Instance.new("Frame")
function line()
	splitline.Name = "Line"
	splitline.Parent = game.CoreGui:WaitForChild("uwuware")
	splitline.BackgroundColor3 = settings.LineColour
	splitline.BorderSizePixel = 0
	splitline.Size = UDim2.new(0, 1917, 0, 5)
end

-- create options

visuals:AddToggle({text = "Fullbright", state = false, callback = function(a)
    warn(a)
    settings.fullbright = a
	if settings.fullbright then
		fullbright()
		notifmsg = "Set Fullbright To ON"
		sendnotification()
	else
		fullbright()
		notifmsg = "Set Fullbright To OFF"
		sendnotification()
	end
end})

local espoptions = visuals:AddFolder("ESP Options")

espoptions:AddToggle({text = "ESP", state = false, callback = function(a)
	settings.ESPEnabled = a
	if settings.ESPEnabled then
	    warn("esp enabled")
	        for _, v in next, Plrs:GetPlayers() do
	                if v ~= MyPlr then
	                    if settings.CharAddedEvent[v.Name] == nil then
	                            settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
	                        if settings.ESPEnabled then
	                            RemoveESP(v)
	                            CreateESP(v)
	                        end
	                        repeat wait() until Char:FindFirstChild("HumanoidRootPart")
	                        	TracerMT[v.Name] = Char.HumanoidRootPart
	                        end)
	                    end
	                RemoveESP(v)
	                CreateESP(v)
	            end
	        end
	       	notifmsg = "Set ESP To ON"
			sendnotification()
		else
	        warn("esp disabled")
	        PlayerESP:ClearAllChildren()
	        ItemESP:ClearAllChildren()
	    	notifmsg = "Set ESP To OFF"
			sendnotification()
	end
end})

espoptions:AddToggle({text = "Chams", state = false, callback = function(a)
	settings.CHAMSEnabled = a
	if settings.CHAMSEnabled then
		warn("chams enabled")
		for _, v in next, Plrs:GetPlayers() do
			if v ~= MyPlr then
				if settings.CharAddedEvent[v.Name] == nil then
						settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
						if settings.CHAMSEnabled then
							RemoveChams(v)
							CreateChams(v)
						end
						repeat wait() until Char:FindFirstChild("HumanoidRootPart")
						TracerMT[v.Name] = Char.HumanoidRootPart
					end)
				end
				RemoveChams(v)
				CreateChams(v)
			end
		end
		notifmsg = "Set Chams To ON"
		sendnotification()
		else
		warn("chams disabled")
		PlayerChams:ClearAllChildren()
		ItemChams:ClearAllChildren()
		notifmsg = "Set Chams To OFF"
		sendnotification()
	end
end})

espoptions:AddToggle({text = "Tracers", state = false, callback = function(a)
	settings.TracersEnabled = a
	if settings.TracersEnabled then
		warn("tracers enabled")
		for _, v in next, Plrs:GetPlayers() do
			if v ~= MyPlr then
				if settings.CharAddedEvent[v.Name] == nil then
					settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
						if settings.TracersEnabled then
							RemoveTracers(v)
							CreateTracers(v)
						end
					end)
				end
				if v.Character ~= nil then
				local Tor = v.Character:FindFirstChild("HumanoidRootPart")
					if Tor then
						TracerMT[v.Name] = Tor
					end
				end
				RemoveTracers(v)
				CreateTracers(v)
			end
		end
		notifmsg = "Set Tracers To ON"
		sendnotification()
		else
		warn("tracers disabled")
		for _, v in next, Plrs:GetPlayers() do
			RemoveTracers(v)
		end
		notifmsg = "Set Tracers To OFF"
		sendnotification()
	end
end})

local espsettings = espoptions:AddFolder("ESP Settings")
espsettings:AddColor({text = "Ally Color", flag = "color", color = Color3.fromRGB(221, 3, 255), callback = function(a)
	settings.Colors.Ally = a
	if settings.ESPEnabled then
		for _, v in next, Plrs:GetPlayers() do
		    if v ~= MyPlr then
		        if settings.CharAddedEvent[v.Name] == nil then
		             settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
		                    repeat wait() until Char:FindFirstChild("HumanoidRootPart")
		                    TracerMT[v.Name] = Char.HumanoidRootPart
		                	end)
		                end
		        RemoveESP(v)
		        CreateESP(v)
		    end
		end
	else return end
	if settings.CHAMSEnabled then
		for _, v in next, Plrs:GetPlayers() do
		    if v ~= MyPlr then
		        if settings.CharAddedEvent[v.Name] == nil then
		             settings.CharAddedEvent[v.Name] = v.CharacterAdded:connect(function(Char)
		                    repeat wait() until Char:FindFirstChild("HumanoidRootPart")
		                    TracerMT[v.Name] = Char.HumanoidRootPart
		                	end)
		                end
			    RemoveChams(v)
				CreateChams(v)
		    end
		end
	else return end
end})

character:AddSlider({text = "WalkSpeed", value = 16, min = 0, max = 150, callback = function(a)
	while wait() do
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = a
	end
end})

character:AddSlider({text = "JumpPower", value = 50, min = 0, max = 500, callback = function(a)
	while wait() do
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = a
	end
end})

character:AddToggle({text = "Fly", callback = function(a)
	settings.vfly = a
	if settings.vfly then
		sFLY()
		notifmsg = "Set Fly To ON"
		sendnotification()
	else
		NOFLY()
		notifmsg = "Set Fly To OFF"
		sendnotification()
	end
end})

character:AddToggle({text = "Noclip", callback = function(a)
	settings.noclip = a
	if settings.noclip then
		notifmsg = "Set Noclip To ON"
		sendnotification()
	    local function NoclipLoop()
	      	if game.Players.LocalPlayer.Character ~= nil then
	          	for _, child in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
		          if child:IsA("BasePart") and child.CanCollide == true then
		              child.CanCollide = false
	          		end
	        	end
	      	end
	    end
	    Noclipping = game:GetService('RunService').Stepped:connect(NoclipLoop)
	  else
	  	notifmsg = "Set Noclip To OFF"
		sendnotification()
	    if Noclipping then
	      	Noclipping:Disconnect()
	    end
	end
end})

miscellaneous:AddToggle({text = "Low Gravity", state = false, callback = function(a)
	settings.lowgravity = a
	if settings.lowgravity then
		notifmsg = "Set Low Gravity To ON"
		sendnotification()
		workspace.Gravity = 60
		notifmsg = "Set Low Gravity To ON"
		sendnotification()
	else
		workspace.Gravity = 196
		notifmsg = "Set Low Gravity To OFF"
		sendnotification()
	end
end})

miscellaneous:AddSlider({text = "Gravity", value = 196, min = 0, max = 196, callback = function(a)
	workspace.Gravity = a
end})

miscellaneous:AddToggle({text = "Annoy Server", state = false, callback = function(a)
	settings.annoyserver = a
	if settings.annoyserver then
		notifmsg = "Annoy Server Enabled"
		sendnotification()
	else
		notifmsg = "Annoy Server Disabled"
		sendnotification()
	end
	while settings.annoyserver do
		wait()
		local Event = game.ReplicatedStorage.Events.ToyEvent
		Event:FireServer("Red Cannon")
		wait(math.random() - math.random() - math.random())
		Event:FireServer("Blue Cannon")
		wait(math.random() - math.random() - math.random())
		Event:FireServer("Yellow Cannon")
		wait(math.random() - math.random() - math.random())
	end
end})

miscellaneous:AddToggle({text = "God Mode", state = false, callback = function(a)
	settings.god = a
	if settings.god then
		godmode()
		notifmsg = "God Mode Enabled"
		sendnotification()
	else
		godmode()
		notifmsg = "God Mode Disabled"
		sendnotification()
	end
end})

local autooptions = miscellaneous:AddFolder("Auto Options")
autooptions:AddToggle({text = "AutoCollect Pollen", callback = function(a)
	settings.autocollectpollen = a
	if settings.autocollectpollen then
		notifmsg = "AutoCollect Pollen Enabled"
		sendnotification()
	    while settings.autocollectpollen do
	    	wait()
	        local autofarmevent = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").ClickEvent;
	        if settings.autocollectpollen == false then return end
	        autofarmevent:FireServer(5)
	    end
	else
		notifmsg = "AutoCollect Pollen Disabled!"
		sendnotification()
	end
end})

autooptions:AddToggle({text = "AutoSell Pollen", callback = function(a)
	settings.autosellpollen = a
	if settings.autosellpollen then
		notifmsg = "AutoSelling Pollen Enabled"
		sendnotification()
		while settings.autosellpollen do
			wait()
			local pollenLbl = game.Players.LocalPlayer.Character:FindFirstChild("ProgressLabel",true)
			local maxpollen = tonumber(pollenLbl.Text:match("%d+$"))
			local currentpollen = tonumber(pollenLbl.Text:match("%d+"))
			if currentpollen>= maxpollen then
				warn("selling pollen")
				wait(0.1)
				game:GetService("Players").LocalPlayer.Character:MoveTo(game:GetService("Players").LocalPlayer.SpawnPos.Value.p)
				wait(1)
				game:GetService("ReplicatedStorage").Events.PlayerHiveCommand:FireServer("ToggleHoneyMaking")
				repeat wait(0.5) until game:GetService("Players").LocalPlayer.CoreStats.Pollen.Value < 1
				wait(8)
				if game:GetService("Players").LocalPlayer.CoreStats.Pollen.Value < 1 then
					warn("sold pollen")
				end
			end
		end
	else
		notifmsg = "AutoSelling Pollen Disabled"
		sendnotification()
	end
end})

autooptions:AddToggle({text = "AutoCollect Tokens", callback = function(a)
	settings.autocollecttokens = a
