miscellaneous:AddButton({text = "Sell Pollen", callback = function()
	notifmsg = "Selling Pollen"
	sendnotification()
	game:GetService("Players").LocalPlayer.Character:MoveTo(game:GetService("Players").LocalPlayer.SpawnPos.Value.p)
	wait(.5)
	game:GetService("ReplicatedStorage").Events.PlayerHiveCommand:FireServer("ToggleHoneyMaking")
	local pollen = game.Players.LocalPlayer.Character:FindFirstChild("ProgressLabel",true).Text
	local currentpollen = pollen:split("/")[1]
	if tonumber(currentpollen) < 1 then
		notifmsg = "Pollen Sold"
		sendnotification()
	end
end})

miscellaneous:AddButton({text = "Collect Treasures", callback = function()
	local function collecttreasure(treasure)
		for i,v in ipairs(treasure:GetChildren()) do
			if v.Parent then
				game.Players.LocalPlayer.Character:MoveTo(v.Position)
			end
		wait(1)
		end
	end
	collecttreasure(workspace.TreasureCollectibles)
	wait(1)
		game.Players.LocalPlayer.Character:MoveTo(game.Players.LocalPlayer.SpawnPos.Value.p)
end})

miscellaneous:AddButton({text = "ReJoin", callback = function()
	game:GetService('TeleportService'):Teleport(game.PlaceId)
end})

