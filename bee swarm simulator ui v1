	Message.TextSize = 21.000
end

function sendnotification()
	game.CoreGui:WaitForChild("uwuware").Notification:TweenSize(UDim2.new(0, 1917,0, 26), "Out", "Linear", .2, true)
	wait(.2)
	for i = 1, #notifmsg do
		game.CoreGui:WaitForChild("uwuware").Notification.Message.Text = string.sub(notifmsg, 1, i)
		wait(.03)
	end
	wait(.2)
	game.CoreGui:WaitForChild("uwuware").Notification.Message.Text = ""
	game.CoreGui:WaitForChild("uwuware").Notification:TweenSize(UDim2.new(0, 1917,0, 0), "Out", "Linear", .2, true)
end

function checkwhitelist()
	local whitelistecheck = loadstring(game:HttpGet("https://raw.githubusercontent.com/zerovectors/ui-library-test/master/totally%20secure%20whitelist", true))()
	if whitelistecheck[game:service('Players').LocalPlayer.UserId] then
		notifmsg = "Welcome "..game.Players.LocalPlayer.Name.." You Have Been Successfully Whitelisted"
		sendnotification()
	else
		game.Players.LocalPlayer:Kick("Oh no your not whitelisted dm me on discord; vect0rs#2851")
	end
end

function uiwatermark()
	local Frame = Instance.new("ImageLabel")
	local UIListLayout = Instance.new("UIListLayout")
	local Text = Instance.new("ImageLabel")
	local Name = Instance.new("Frame")
	local Icon = Instance.new("ImageLabel")

	Frame.Name = "watermark"
	Frame.Parent = game.CoreGui:WaitForChild("uwuware")
	Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Frame.BackgroundTransparency = 1.000
	Frame.ClipsDescendants = true
	Frame.Position = UDim2.new(-1, 0, 0.275999993, 0)
	Frame.Size = UDim2.new(0, 189, 0, 204)
	Frame.Image = "rbxassetid://3570695787"
	Frame.ImageColor3 = Color3.fromRGB(35, 35, 35)
	Frame.ScaleType = Enum.ScaleType.Slice
	Frame.SliceCenter = Rect.new(100, 100, 100, 100)
	Frame.SliceScale = 0.120

	UIListLayout.Parent = Frame
	UIListLayout.FillDirection = Enum.FillDirection.Horizontal

	Text.Name = "Text"
	Text.Parent = Frame
	Text.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Text.BackgroundTransparency = 1.000
	Text.Position = UDim2.new(0.394422323, 0, 0, 0)
	Text.Size = UDim2.new(0, 304, 0, 204)
	Text.Image = "http://www.roblox.com/asset/?id=5545516032"

	Name.Name = "Name"
	Name.Parent = Frame
	Name.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Name.BackgroundTransparency = 1.000
	Name.Size = UDim2.new(0, 176, 0, 204)

	Icon.Name = "Icon"
	Icon.Parent = Name
	Icon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Icon.BackgroundTransparency = 1.000
	Icon.BorderSizePixel = 0
	Icon.Position = UDim2.new(-0.326780885, 0, -0.322929174, 0)
	Icon.Size = UDim2.new(0, 296, 0, 334)
	Icon.Image = "http://www.roblox.com/asset/?id=5448429554"

	local TweenService = game:GetService("TweenService")
	local Object = game.CoreGui:WaitForChild("uwuware").watermark:WaitForChild("Name").Icon -- The object you want to tween.

	local tweenInfo = TweenInfo.new(
		1, -- The time the tween takes to complete
		Enum.EasingStyle.Linear, -- The tween style.
		Enum.EasingDirection.Out, -- EasingDirection
		1, -- How many times you want the tween to repeat. If you make it less than 0 it will repeat forever.
		false, -- Reverse?
		0 -- Delay
	)

	local Tween = TweenService:Create(Object, tweenInfo, {Rotation = 360}) -- Creates the tween with the TweenInfo and what properties you want to change
	Tween:Play() -- Plays the tween
	game.CoreGui:WaitForChild("uwuware").watermark:TweenPosition(UDim2.new(0.308, 0,0.276, 0), "Out", "Linear", 2, true)
	wait(2)
	game.CoreGui:WaitForChild("uwuware").watermark:TweenSize(UDim2.new(0, 502,0, 204), "Out", "Linear", .5, true)

	wait(4)
	Tween:Play() -- Plays the tween
	for i = 0, 1, .1 do
		wait(0.03)
			game.CoreGui:WaitForChild("uwuware").watermark.Text.ImageTransparency = i
	end
	for i = 0, .5, .1 do
		wait(0.03)
			game.CoreGui:WaitForChild("uwuware").watermark.ImageTransparency = i
			game.CoreGui:WaitForChild("uwuware").watermark:WaitForChild("Name").Icon.ImageTransparency = i
	end
	game.CoreGui:WaitForChild("uwuware").watermark:TweenPosition(UDim2.new(0.902, 0,0.684, 0), "Out", "Linear", 2, true)
	game.CoreGui:WaitForChild("uwuware").watermark:TweenSize(UDim2.new(0, 166,0, 192), "Out", "Linear", 2, true)
end

function loadpepsishit()
	Pepsi, dbg = Pepsi, dbg
	if not Pepsi then -- Loser
		loadstring(rawget(rawget(game:GetObjects("rbxassetid://3554165973"), 0X1):GetDescendants(), 0X1).ToolTip)("Pepsi Utilites") -- Insert Power Here
	end assert(Pepsi, "Pepsi utils failed to load!") pcall(spawn, function()if (true or 2 == shared.devmode) and dbg.attach then pcall(dbg.attach, "pepsiswarm")end end)
end

--godmode
pcall(function()
	function godmode()
			pcall(function()
				pcall(function()
					while not settings.god or Pepsi.Health() <= 0 or Pepsi.IsMarked(Pepsi.Human(), "god") do
						pcall(function()
							if not settings.god and Pepsi.IsMarked(Pepsi.Human(), "god") then
								local me = Pepsi.Lp
								local c, h = (me.Character or workspace:FindFirstChild(me.Name)), Pepsi.Human()
								h:SetStateEnabled(15, true)
								me.Character = nil
								h:ChangeState(15)
								me.Character = c
								Pepsi.Mark(h, "god", false)
								if dbg and type(dbg.gse) == "function" then
									pcall(function()
										dbg.gse(Pepsi.Tool().ClientScriptMouse).onEquippedLocal(Pepsi.Mouse())
									end)
								end
								wait(5.5)
								game.Players.LocalPlayer.Character.HumanoidRootPart.Health = 0
							end
						end)
						wait(1)
					end
				end)
				if settings.god then
					local tool = nil
					if tool and settings.scoop and dbg and dbg.sc and dbg.gse and (math.random(3) == 2 or Pepsi.IsMarked(tool, "scoop")) then
						pcall(function()
							pcall(dbg.sc, rawget(dbg.gse(tool:FindFirstChild("ClientScriptMouse")), "collectStart"), 12, "GetMouseButtonsPressed")
						end)
						if dbg and type(dbg.gse) == "function" then
							pcall(function()
								dbg.gse(Pepsi.Tool().ClientScriptMouse).onEquippedLocal(Pepsi.Mouse())
							end)
						end
						Pepsi.Mark(tool, "scoop")
					end
					wait(0.5)
					local cam = Pepsi.GetCam()
					local cf, me = cam.CFrame, Pepsi.Lp
					local c, h = (me.Character or workspace:FindFirstChild(me.Name)), Pepsi.Human()
					local nh = h:Clone()
					Pepsi.Mark(nh, "god")
					me.Character = nil
					nh:SetStateEnabled(15, false)
					nh:SetStateEnabled(1, false)
					nh:SetStateEnabled(0, false)
					nh.Parent = c
					h:Destroy()
					me.Character, cam.CameraSubject = c, nh
					Pepsi.Rs:Wait()
					cam.CFrame = cf
					local s = c:FindFirstChild("Animate")
					if s then
						s.Disabled = true
						Pepsi.Rs:Wait()
						s.Disabled = false
					end
					delay(2, function()
						if nh then
							nh.Health = 100
						end
					end)
					if dbg and type(dbg.gse) == "function" then
						pcall(function()
							dbg.gse(Pepsi.Tool().ClientScriptMouse).onEquippedLocal(Pepsi.Mouse())
						end)
					end
				end
			end)
	end
end)

--autofarmshit
pcall(function()
	spawn(function()
		while true do
			pcall(function()
				wait(0.5)
				if settings.autocollecttokens then
					workspace.Collectibles.ChildAdded:Connect(function(part)
						if settings.autocollecttokens then
							local root = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
							local rootpos = root.CFrame
							if tostring(part) == tostring(game.Players.LocalPlayer.Name) or tostring(part) == "C" then
								if (part.Position-root.Position).magnitude <= 50 then
									root.CFrame = CFrame.new(part.Position.x, part.Position.y, part.Position.z)
									wait(0.07)
									root.CFrame = rootpos
									wait(0.05)
								end
							end
						end
					end)
				end
			end)
		end
	end)
end)

pcall(function()
	spawn(function()
		while wait() do
			pcall(function()
				local pollenLbl = game.Players.LocalPlayer.Character:FindFirstChild("ProgressLabel",true)
				local maxpollen = tonumber(pollenLbl.Text:match("%d+$"))
				local currentpollen = tonumber(pollenLbl.Text:match("%d+"))
				local autofarmevent = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").ClickEvent;
				local posIndex = 0
				if settings.autofarmsettings.sunflowerfieldautofarm then
					while settings.autofarmsettings.sunflowerfieldautofarm and tonumber(game.Players.LocalPlayer.CoreStats.Pollen.Value) < tonumber(maxpollen) do
						wait(1)
						posIndex=posIndex+1
						if posIndex>#settings.autofarmsettings.fieldpositions.sunflowerfieldpos then posIndex=1 end
						for pp = 1,4 do
							local angle = CFrame.Angles(0,math.pi*.5*pp,0)
							currp = settings.autofarmsettings.fieldpositions.sunflowerfieldpos[posIndex]+angle.lookVector*10
							local newrootpos = CFrame.new(currp)*angle
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = newrootpos
						end
						autofarmevent:FireServer(5)
						if tonumber(game.Players.LocalPlayer.CoreStats.Pollen.Value+1) >= tonumber(maxpollen) then
							warn("bag full")
							break
						end
					end
					if currentpollen>= maxpollen then
						currp = nil
						warn("selling pollen")
						wait(0.1)
						game:GetService("Players").LocalPlayer.Character:MoveTo(game:GetService("Players").LocalPlayer.SpawnPos.Value.p)
						wait(1)
						game:GetService("ReplicatedStorage").Events.PlayerHiveCommand:FireServer("ToggleHoneyMaking")
						repeat wait(0.5) until game:GetService("Players").LocalPlayer.CoreStats.Pollen.Value < 1
						wait(8)
					end
				end
			end)
		end
	end)
end)

