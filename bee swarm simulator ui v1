uisettings:AddToggle({text = "Cursor", callback = function(a)
	settings.Cursorenabled = a
	if settings.Cursorenabled then
		Cursor.Visible = true
		warn("cursor enabled")
		notifmsg = "Set Cursor To ON"
		sendnotification()
		else
		Cursor.Visible = false
		warn("cursor disabled")
		notifmsg = "Set Cursor To OFF"
		sendnotification()
	end
end})

uisettings:AddColor({text = "Cursor Color", flag = "color", color = Color3.fromRGB(221, 3, 255), callback = function(a)
	settings.CursorColor = a
	Cursor.ImageColor3 = settings.CursorColor
end})

if game.Players.LocalPlayer.UserId == 278723686 then
	uisettings:AddToggle({text = "UI Watermark", state = true, callback = function(a)
		game.CoreGui:WaitForChild("uwuware").watermark.Visible = a
	end})
elseif game.Players.LocalPlayer.UserId == 425451555 then
	uisettings:AddToggle({text = "UI Watermark", state = true, callback = function(a)
		game.CoreGui:WaitForChild("uwuware").watermark.Visible = a
	end})
end

local toplineoptions = uisettings:AddFolder("Topbar")

toplineoptions:AddColor({text = "Line Colour", flag = "color", color = Color3.fromRGB(221, 3, 255), callback = function(a)
	settings.LineColour = a
	splitline.BackgroundColor3 = settings.LineColour
end})

toplineoptions:AddSlider({text = "Transparency", value = 0, min = 0, max = 100, callback = function(a)
	splitline.BackgroundTransparency = a / 100
end})

toplineoptions:AddSlider({text = "Line Thickness", value = 1, min = 1, max = 4, callback = function(a)
	splitline.Size = UDim2.new(0, 1917, 0, a*3)
end})

uisettings:AddBind({text = "Toggle UI", key = "Q", callback = function()
	library:Close()
end})

credits:AddLabel({text = "Vect0rs; scripts"})
credits:AddLabel({text = "Pepsi Man/PepsiSwarm; scripts"})
credits:AddLabel({text = "Jan; ui library"})
credits:AddLabel({text = "Smail; helpful, cool guy"})

Plrs.PlayerAdded:Connect(function(Plr)
		if settings.CharAddedEvent[Plr.Name] == nil then
			settings.CharAddedEvent[Plr.Name] = Plr.CharacterAdded:connect(function(Char)
				if settings.TracersEnabled then
					CreateTracers(Plr)
				end
				if settings.CHAMSEnabled then
					CreateChams(Plr)
				end
				if settings.ESPEnabled then
					RemoveESP(Plr)
					CreateESP(Plr)
				end
				repeat wait() until Char:FindFirstChild("HumanoidRootPart")
				TracerMT[Plr.Name] = Char.HumanoidRootPart
			end)
		end
end)

Plrs.PlayerRemoving:Connect(function(Plr)
		if settings.CharAddedEvent[Plr.Name] ~= nil then
			settings.CharAddedEvent[Plr.Name]:Disconnect()
			settings.CharAddedEvent[Plr.Name] = nil
		end
		RemoveTracers(Plr)
		RemoveChams(Plr)
		RemoveESP(Plr)
		TracerMT[Plr.Name] = nil
end)

library:Init()
uiwatermark()
hintmsg:Remove()
line()
createnotification()
checkwhitelist()
notifmsg = "[Vect0rs Hub] Loaded; Bee Simulator UI";
sendnotification()
notifmsg = "Version; "..version
sendnotification()
loadpepsishit()
createinfothingy()
