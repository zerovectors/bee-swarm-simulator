local hintmsg = Instance.new("Hint", Workspace)
hintmsg.Text = "Initializing UI This can take up to 30 seconds"
game:GetService("ScriptContext").Error:Connect(function(message)
	print("Reason: "..message)
	hintmsg.Text = "Initializing Failed, Error; "..message
	if message == "HttpError: ConnectFail" then
		hintmsg.Text = "Initializing Failed!| Error; "..message.." | Fix; Re-Execute or ReJoin"
	end
	wait(6)
	hintmsg:Remove()
end)
